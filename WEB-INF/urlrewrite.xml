<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite default-match-type="wildcard">
    <rule>
            <from>/cmsadmin/*</from>
            <to>/cmsadmin/$1</to>
    </rule>
    <rule>
            <from>/app-*</from>
            <to>$1.jsp</to>
    </rule>
    <rule>
            <from>/*</from>
            <to>index.jsp</to>
    </rule>
</urlrewrite>
